package com.example.demo;

import org.springframework.stereotype.Service;
import com.example.demo.Repository;
import java.util.*;

@Service 

public class UserService{
	
	private Repository rep = null;
	
	
	
	public UserService() {
		
	}
	
	public List<Entity> getAll(){
		return rep.findAll();
	}
	
	
	
	
	
}